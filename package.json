{
  "name": "s3-url-exists",
  "version": "1.0.0",
  "description": "Check if URL exists in S3",
  "main": "index.js",
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "test": "mocha test/*.test.js --exit --timeout 555555",
